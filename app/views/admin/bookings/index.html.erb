<div class="service-banner">
  <div class="banner-content">
    <h1><%= current_user.first_name %>'s Bookings List</h1>
  </div>
</div>

<---
<div class="container show-grey">
  <div class="row text-center">
    <h1><%= current_user.first_name %>'s bookings list</h1>
  </div>
  <div class="row">
    <div class="col-xs-12 text-center">
      <ul class="list-unstyled">
        <% @bookings.each do |booking| %>
          <li>
            <h3>
              <%= link_to booking_path(booking) do %>
                <%= booking.service.name %>: <%= booking.title %>
                , <%= booking.status %>
                -
                <%= link_to 'Delete', booking, method: :delete %>
              <% end %>
            </h3>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <table class="table">
    <thead class="thead-default">
      <tr>
        <th>Booked Services</th>
        <th>Status</th>
        <th>Update</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>Mark</td>
        <td>Otto</td>
        <td>@mdo</td>
      </tr>
    </tbody>
    </table>
  </div>
</div>